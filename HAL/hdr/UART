; not used any longer .. JB  20/2/12

UART011_DR      * 0
UART011_RSR     * 4
UART011_ECR     * 4
UART011_CR      * &30
UART011_FR      * &18
UART011_FBRD    * &28
UART011_IBRD    * &24
UART011_LCRH    * &2C

                END
